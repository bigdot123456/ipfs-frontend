{
  "name": "ipfs-frontend",
  "version": "0.7.0",
  "dependencies": {
    "@melonproject/melon.js": "0.7.6",
    "@melonproject/protocol": "0.7.2-alpha.1",
    "babel-polyfill": "^6.23.0",
    "bignumber.js": "^5.0.0",
    "bn.js": "^4.11.8",
    "ethers": "^2.1.3",
    "flat": "^4.0.0",
    "gulp": "^4.0.0",
    "history": "^4.7.2",
    "moment": "^2.20.1",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-cntdwn": "^0.3.0",
    "react-dom": "^16.2.0",
    "react-highlighter": "^0.4.2",
    "react-modal": "^3.1.10",
    "react-redux": "^5.0.6",
    "react-scripts": "^1.0.17",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-first-router": "0.0.4-rudy",
    "redux-first-router-link": "^1.4.2",
    "redux-form": "^7.2.0",
    "redux-saga": "^0.16.0",
    "semantic-ui-react": "^0.77.1",
    "slugify": "^1.2.9"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run-script build:postprocess",
    "build:dev": "react-scripts build && npm run rename-absolute-path",
    "build:postprocess": "npm run-script delete-maps && npm run-script rename-absolute-path",
    "delete-maps": "find ./build -name '*.map' -delete",
    "rename-absolute-path": "sed -i -e 's@/static/js@./static/js@g' ./build/index.html",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --record",
    "deploy": "ipfs add -r public",
    "deploy:gh-pages": "npm run build:dev && gh-pages -d build",
    "eject": "react-scripts eject",
    "lint": "eslint ./",
    "semantic:build": "cd public/semantic && gulp build",
    "semantic:watch": "cd public/semantic && gulp",
    "storybook:build": "build-storybook -s public -o .out -c tests/storybook",
    "storybook": "start-storybook -p 9001 -c tests/storybook -s public",
    "test": "react-scripts test --env=jsdom",
    "check": "npm-check",
    "check:update": "npm-check -u ",
    "snyk-protect": "snyk protect"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.3",
    "@storybook/addon-info": "^3.3.3",
    "@storybook/addon-knobs": "^3.3.3",
    "@storybook/react": "^3.3.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cypress": "^1.4.1",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-security": "^1.4.0",
    "gh-pages": "^1.1.0",
    "npm-check": "^5.5.2",
    "snyk": "^1.68.0",
    "wait-on": "^2.0.2"
  },
  "snyk": true
}
